{
  "project": {
    "version": "1.0.0-beta.19-3",
    "name": "web-admin-preview"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "assets/bootstrap/css/bootstrap.min.css",
        "assets/dist/css/AdminLTE.min.css",
        "assets/dist/css/skins/skin-black.min.css",
        "assets/plugins/iCheck/square/blue.css",
        "styles.scss"
      ],
      "scripts": [
        "assets/plugins/jQuery/jquery-2.2.3.min.js",
        "assets/bootstrap/js/bootstrap.min.js",
        "assets/dist/js/app.min.js",
        "assets/plugins/iCheck/icheck.min.js",
        "assets/scripts.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [
  ],
  "packages": [
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
